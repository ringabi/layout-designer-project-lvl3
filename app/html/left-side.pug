        img.text-black-50(src="./app/images/icons/telephone.svg" alt="telephone" width="16" height="16")
      li.list-group-item.d-flex.justify-content-between.align-items-center.py-3.border-right-0.border-left-0
        div.d-flex.flex-column.small
          span.text-secondary Email
          span.text-dark nikita.m@hexlet.io
        img.text-black-50(src="./app/images/icons/envelope.svg" alt="envelope" width="16" height="16")
